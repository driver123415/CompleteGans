-- Script Claim 1 - 50
for i = 1, 50 do
    local args = {
        [1] = {
            [1] = {
                [1] = i
            },
            [2] = codes[1] -- Menggunakan kode pertama dari tabel codes
        }
    }

    game:GetService("ReplicatedStorage").dataRemoteEvent:FireServer(unpack(args))
end
